include ./../../../general.ini

[Config WarmupMeasurement]
repeat = 30
sim-time-limit = 5d
SimNetwork.a.runway.throughputCheckInterval = 25min
SimNetwork.wg.interArrivalTime = exponential(25min)
SimNetwork.a.runway.landingTime = truncnormal(5min, 0.5min)
SimNetwork.a.runway.takeoffTime = truncnormal(5min, 0.5min)
SimNetwork.a.parkingLot.parkingDelay = truncnormal(45min, 5 min)

[Config QueueMeasurement]
repeat = 70
sim-time-limit = 11d
warmup-period = 20h
SimNetwork.a.runway.throughputCheckInterval = 25min
SimNetwork.wg.interArrivalTime = exponential(25min)
SimNetwork.a.runway.landingTime = truncnormal(5min, 0.5min)
SimNetwork.a.runway.takeoffTime = truncnormal(5min, 0.5min)
SimNetwork.a.parkingLot.parkingDelay = truncnormal(45min, 5 min)
