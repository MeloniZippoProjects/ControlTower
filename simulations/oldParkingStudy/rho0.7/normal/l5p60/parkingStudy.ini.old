include ./../../../../general.ini

[Config WarmupMeasurement]
repeat = 30
sim-time-limit = 25d
SimNetwork.a.runway.throughputCheckInterval = 14.286min
SimNetwork.wg.interArrivalTime = exponential(14.286min)
SimNetwork.a.runway.landingTime = truncnormal(5min, 0.5min)
SimNetwork.a.runway.takeoffTime = truncnormal(5min, 0.5min)
SimNetwork.a.parkingLot.parkingDelay = truncnormal(60min, 6min)

[Config ParkingMeasurement]
repeat = 150
sim-time-limit = 25d
warmup-period = 1.4d
SimNetwork.a.runway.throughputCheckInterval = 14.286min
SimNetwork.wg.interArrivalTime = exponential(14.286min)
SimNetwork.a.runway.landingTime = truncnormal(5min, 0.5min)
SimNetwork.a.runway.takeoffTime = truncnormal(5min, 0.5min)
SimNetwork.a.parkingLot.parkingDelay = truncnormal(60min, 6min)
