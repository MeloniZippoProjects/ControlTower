include ./../../../general.ini

[Config WarmupMeasurement]
repeat = 30
sim-time-limit = 5d
SimNetwork.a.runway.throughputCheckInterval = 100min
SimNetwork.wg.interArrivalTime = 100min
SimNetwork.a.runway.landingTime = exponential(15min)
SimNetwork.a.runway.takeoffTime = exponential(5min)
SimNetwork.a.parkingLot.parkingDelay = exponential(60min)

[Config QueueMeasurement]
repeat = 70
sim-time-limit = 14d
warmup-period = 3.5d
SimNetwork.a.runway.throughputCheckInterval = 100min
SimNetwork.wg.interArrivalTime = 100min
SimNetwork.a.runway.landingTime = exponential(15min)
SimNetwork.a.runway.takeoffTime = exponential(5min)
SimNetwork.a.parkingLot.parkingDelay = exponential(60min)
